@Article{Morra1999,
  author   = {Linda G. Morra and Amy C. Friedlander},
  title    = {Case study evaluations},
  year     = {1999},
  abstract = {The Operations Evaluation Department (OED) uses case studies for in-depth consideration of the results of a project or group of projects or to illustrate given points. There is considerable confusion about what is and is not a case study, what is case study methodology, what use can be made of them, and how they should be written-up for dissemination. This report attempts to clarify these issues. It discusses the size of the case study, methods in obtaining and analyzing data in case studies, how to select the number of cases or instances, case study types and site selection including explanatory and descriptive case studies, cases studies that combine methodology, and the distinction between case studies thus far described and case studies used for teaching purposes.},
  url      = {https://www.semanticscholar.org/paper/d39a820ea820ae2d6197a79711a52c6d9bea1255},
}

